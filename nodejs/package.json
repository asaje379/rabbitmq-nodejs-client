{
  "name": "@asaje/rabbitmq-node-client",
  "version": "0.1.12",
  "description": "Rabbitmq client for nodejs applications",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/main.ts",
    "build": "tsc && esbuild src/main.ts --bundle --minify --platform=node --packages=external --outdir=dist",
    "start": "node dist/main.js"
  },
  "keywords": [
    "rabbit mq",
    "message broker"
  ],
  "author": "Salem AFFA",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/node": "^22.5.0",
    "esbuild": "^0.23.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "nanoid": "^3.3.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asaje379/rabbitmq-nodejs-client/tree/main/nodejs"
  }
}
